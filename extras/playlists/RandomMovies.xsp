<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Random Movies</name>
    <match>all</match>
    <group>none</group>
   	<rule field="genre" operator="isnot">
        <value>children</value>
    </rule>
	<limit>25</limit>
    <order direction="ascending">random</order>
</smartplaylist>
